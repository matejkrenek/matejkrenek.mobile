{
  "expo": {
    "entryPoint": "./src/index.tsx",
    "name": "OneTwo",
    "slug": "OneTwo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "taskmanager.matejkrenek",
      "buildNumber": "1.0.0",
      "supportsTablet": true

    },
    "android": {
      "package": "taskmanager.matejkrenek",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    }
  }
}
